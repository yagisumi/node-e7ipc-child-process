  "description": "Node.js child_process communication wrapper similar to Electron v7.",
  "keywords": [],
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "lib/e7ipc-child-process",
  "module": "lib/e7ipc-child-process.mjs",
  "types": "lib/e7ipc-child-process.d.ts",
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "jest": "jest",
    "build": "run-s clean build:*",
