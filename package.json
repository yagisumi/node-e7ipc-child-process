    "build:cjs": "rollup -c -f cjs -o ./lib/e7ipc-child-process.js",
    "build:mjs": "rollup -c -f es -o ./lib/e7ipc-child-process.mjs",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write **/*.{ts,js,json}",
