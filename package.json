    "@typescript-eslint/parser": "^2.27.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
